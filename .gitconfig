[filter "lfs"]
	process = git-lfs filter-process
	required = true
	clean = git-lfs clean -- %f
	smudge = git-lfs smudge -- %f
[user]
	name = Natsune Mochizuki
	email = me@natsuneko.cat
[credential "https://git-lfs.natsuneko.moe"]
	provider = generic
[credential "https://huggingface.co"]
	provider = generic
[init]
	defaultBranch = main
[commit]
	gpgsign = true
[tag]
	gpgsign = true
